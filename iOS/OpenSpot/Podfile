# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'OpenSpot' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  # Pods for OpenSpot
pod 'FirebaseUI/Auth'
pod 'FirebaseUI/Phone'
pod 'Firebase/Firestore'
pod 'Firebase/Core'
pod 'GoogleMaps'
pod 'GooglePlaces'

  target 'OpenSpotTests' do
    inherit! :search_paths
    # Pods for testing
pod 'FirebaseUI/Auth'
  end


  target 'OpenSpotUITests' do
    inherit! :search_paths
    # Pods for testing
pod 'FirebaseUI/Auth'
pod 'FirebaseUI/Phone'
pod 'Firebase/Firestore'
pod 'Firebase/Core'
  end
end


